*     Predefined Device Stanza(s) for native serial port1
PdDv:
	type           = s1a
	class          = adapter
	subclass       = isa_sio
	prefix         = sa
	devid          = "PNP05011"
	base           = 0
	has_vpd        = 0
	detectable     = 1
	chgstatus      = 0
	bus_ext        = 0
	fru            = 2
	led            = 2086
	setno          = 39
	msgno          = 1
	catalog        = devices.cat
	DvDr           = isa/rsdd_rspc
	Define         = /usr/lib/methods/define_rspc
	Configure      = /usr/lib/methods/cfgasync_rspc
	Change         = /usr/lib/methods/chggen
	Unconfigure    = /usr/lib/methods/ucfgdevice
	Undefine       = /usr/lib/methods/undefine
	Start          = ""
	Stop           = ""
	inventory_only = 0
	uniquetype     = adapter/isa_sio/s1a

*     Predefined Attribute Stanza(s) for native serial port1
PdAt:
	uniquetype = adapter/isa_sio/s1a
	attribute  = bus_io_addr
	deflt      = 0x3f8
	values     = 0x3f8
	width      = 0x08
	type       = "O11"
	generic    = D
	rep        = nl
	nls_index  = 3

PdAt:
	uniquetype = adapter/isa_sio/s1a
	attribute  = bus_intr_lvl
	deflt      = 4
	values     = 4
	width      = ""
	type       = N
	generic    = D
	rep        = nl
	nls_index  = 5

PdAt:
	uniquetype = adapter/isa_sio/s1a
	attribute  = intr_priority
	deflt	   = 1
	values	   = 1
	width	   = ""
	type	   = P
	generic	   = D
	rep	   = nl
	nls_index  = 6

PdAt:
	uniquetype = adapter/isa_sio/s1a
	attribute  = frequency
	deflt      = 115200
	values     = 115200
	width      = ""
	type       = R
	generic    = ""
	rep        = nl
	nls_index  = 0

PdAt:
