	uniquetype = "adapter/isa_sio/s2a"
	attribute = "bus_io_addr"
	deflt = "0x2f8"
	values = "0x2f8"
	width = "0x08"
	type = "O"
	generic = "D"
	rep = "nl"
	nls_index = 3

