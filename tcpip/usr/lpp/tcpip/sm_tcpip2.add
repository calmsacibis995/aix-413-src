# =========================== CHANGE SLIP INTERFACE ===========================
sm_cmd_hdr:
	id			= "chinetsl"
	option_id		= "_chinetsl"
	has_name_select 	= "y"
	name			= "Change / Show a Serial Line Network Interface"
	name_msg_file		= "smit.cat"
	name_msg_set		= 4
	name_msg_id		= 312
	cmd_to_exec		= "\
function csl\n\
{\n\
IP=$2\n\
PARM=`echo $@ | sed -e \"s/[\t ]-a[\t ]dialstring=.*//\"`\n\
BD=`echo $@ | sed -e \"s/.*[\t ]-a[\t ]baudrate=//\"\
	-e \"s/[\t ].*//\"`\n\
STATE=`echo $@ | sed -e \"s/.*[\t ]-a[\t ]state=//\"\
	-e \"s/[\t ].*//\"`\n\
for i in \"$@\"\n\
do\n\
	SLIP_DIAL_STRING=\"$i\"\n\
	shift\n\
done\n\
export SLIP_DIAL_STRING\n\
DS=\"$SLIP_DIAL_STRING\"\n\
if [  -z \"$DS\"  -a  ! -z \"$BD\" ]\n\
then\n\
	SLIP_DIAL_STRING=\"\"\n\
fi\n\
if [ -z \"$BD\" -a  ! -z \"$DS\" ]\n\
then\n\
	echo \"Missing Baudrate\"\n\
	exit 1\n\
fi\n\
strinfo -m | grep slip>/dev/null\n\
SD=$?\n\
if [ $SD != 0 ]\n\
then\n\
	strload -m /usr/lib/drivers/slip\n\
fi\n\
SD=$?\n\
if [ $SD != 0 ]\n\
then\n\
	echo \"Cannot load slip discipline.\"\n\
	exit 1\n\
fi\n\
OP=`lsattr -E -l $IP -F \"value\" -a ttyport`\n\
ID=`ps -ef | grep \"slattach[\t ].*$OP[\t ]\" |\n\
    awk '{ print $2 }'`\n\
if [ ! -z \"$ID\" ]\n\
then\n\
\tkill -15 $ID\n\
fi\n\
chdev $PARM\n\
if [ $? != 0 ]\n\
then\n\
	exit 1\n\
fi\n\
if [ $STATE = detach -o $STATE = down ]\n\
then\n\
        exit 0\n\
fi\n\
TP=`lsattr -E -l $IP -F \"value\" -a ttyport`\n\
if [ -z \"$BD\" -a  -z \"$DS\" ]\n\
then\n\
	eval slattach $TP\n\
	sleep 30\n\
else\n\
	eval DST=\\\'$SLIP_DIAL_STRING\\\'\n\
	eval slattach $TP $BD \\\'$DST\\\'\n\
	sleep 60\n\
fi\n\
}\n\
csl"
	ask			= "y"
	exec_mode		= "p"
	ghost			= "n"
	cmd_to_discover 	= "function cmd_to_discover\n{\nlsattr -E -l $1 -O\n}\ncmd_to_discover"
	cmd_to_discover_postfix   = "_rawname"
	name_size		= 0
	value_size		= 0
	help_msg_id		= 0822020
	help_msg_loc		= ""
 
 
