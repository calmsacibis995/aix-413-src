# =========================== SET HOST NAME ===================================
sm_cmd_hdr:
	id			= "mkhostname"
	option_id		= "_mkhostname"
	has_name_select 	= ""
	name			= "Set Hostname\n\nPlease refer to Help for information\nconcerning hostname / INTERNET address mapping"
	name_msg_file		= "smit.cat"
	name_msg_set		= 4
	name_msg_id		= 99
        cmd_to_exec             = "function cmd_to_exec {
        mkdev -t inet
        chdev -l inet0 -a hostname=$1
        if [ -x /usr/sbin/hostid ]
        then
                /usr/sbin/hostid `hostname`
                if [ -x /usr/lib/lpd/pio/etc/piodmgr ]
                then
                        /usr/lib/lpd/pio/etc/piodmgr -c > /dev/null 2>&1
                fi
        else
                echo "Could not find /usr/sbin/hostid"

        fi
        }; cmd_to_exec"
	ask			= "n"
	exec_mode		= "p"
	ghost			= "n"
	cmd_to_discover 	= "function cmd_host\n{\necho \"#_rawname\"\nhostname\n}\ncmd_host"
	cmd_to_discover_postfix   = ""
	name_size		= 0
	value_size		= 0
	help_msg_id		= 0822003
	help_msg_loc		= ""
 
 
