sm_cmd_hdr:
	id			= "chif1"
	option_id		= "_chif1"
	has_name_select 	= "y"
	name			= "Network Interface Drivers"
	name_msg_file		= "smit.cat"
	name_msg_set		= 4
	name_msg_id		= 10
	cmd_to_exec		= "function cmd_to_exec\n{\nif [ -z \"$3\" ]\nthen\n\texit\nfi\neval chdev $*\n}\ncmd_to_exec"
	ask			= "n"
	exec_mode		= "p"
	ghost			= "n"
	cmd_to_discover		= "function cmd_to_discover\n\
{\n\
# this enable the mtu size for 4 - 16 megabit ring speeds to be set\n\
# in the predefined attribute\n\
IF=`echo $1 | cut -c1-2`\n\
if [ \"$IF\" = \"tr\" ]\n\
then\n\
	TOKADP=tok`echo $1 | cut -c3`\n\
	RINGSPEED=`lsattr -E -l $TOKADP -F \"value\" -a ring_speed`\n\
	if [ $RINGSPEED = "autosense" ]\n\
	then\n\
		CFG_RING_SPEED=`odmget -q \"name=$TOKADP and attribute=cfg_ring_speed\" CuAt | grep value | sed -e 's/\"//g' | awk '{ print $3 }'`\n\
		if [ \"$CFG_RING_SPEED\" = \"\" ]\n\
		then\n\
			RINGSPEED=4\n\
		else\n\
			RINGSPEED=$CFG_RING_SPEED\n\
		fi\n\
	fi\n\
	odmget -q\"uniquetype=if/TR/tr and attribute=mtu_$RINGSPEED\" PdAt                      | sed -e \"s/mtu_$RINGSPEED/mtu/\" >/tmp/tmpp$$ 2>/dev/null\n\
	if [ $? = 0 ] \n\
	then\n\
		odmchange -o PdAt -q\"uniquetype=if/TR/tr and attribute=mtu\" /tmp/tmpp$$ 2>&1 >/dev/null\n\
	fi\n\
	if [ -f /tmp/tmpp$$ ]\n\
	then\n\
		rm /tmp/tmpp$$ 2>&1 >/dev/null\n\
	fi\n\
fi\n\
lsattr -E -l $1 -O\n\
}\n\
cmd_to_discover"
