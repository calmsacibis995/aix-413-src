	name_msg_set		= 4
	name_msg_id		= 263
	cmd_to_exec		= "/usr/sbin/chsubserver -r inetd -C /etc/inetd.conf -a "
	ask			= "n"
	exec_mode		= "p"
	ghost			= "n"
	cmd_to_discover 	= "function cmddisc_conf\n\
{\n\
SV=`echo $1 | awk '{ print $1 }'`\n\
PR=`echo $1 | awk '{ print $2 }'`\n\
egrep \"$SV.*.$PR\" /etc/inetd.conf > /dev/null 2>&1\n\
RC=$?\n\
if [ $RC != 1 ]\n\
then\n\
egrep \"$SV.*.$PR\" /etc/inetd.conf | sed -e 's/^#//' | awk '{ printf(\"#service:socket:proto:wait:user:prog:args\\n%s:%s:%s:%s:%s:%s:%s:\\n\", $1, $2, $3, $4, $5, $6, $7) }'\n\
else\n\
echo \"#service:socket:proto:wait:user:prog:args\\n$SV:stream:$PR:nowait:::\"\n\
fi\n\
}\n\
cmddisc_conf"
