sm_cmd_hdr:
	id			= "mkinetsl"
	option_id		= "_mkinetsl"
	has_name_select 	= ""
	name			= "Add a Serial Line INTERNET Network Interface"
	name_msg_file		= "smit.cat"
	name_msg_set		= 4
	name_msg_id		= 19
	cmd_to_exec		= "\n\
function cmdsl\n{\n\
echo $@ | grep \"[\t ]-a[\t ]state=up\" >/dev/null\n\
UP=$?\n\
echo $@ | grep \"[\t ]-a[\t ]dialstring=\" >/dev/null\n\
DS=$?\n\
echo $@ | grep \"[\t ]-a[\t ]baudrate=\" >/dev/null\n\
BD=$?\n\
PARM=`echo $@ | sed -e \"s/[\t ]-a[\t ]dialstring=.*//\"`\n\
UNIT=`echo $@ | sed -e 's/.*ttyport=tty\\([0-9]*\\).*/\\1/'` \n\
if [ $DS = 0  -a  $BD != 0 ]\n\
then\n\
	echo \"Missing Baudrate.\"\n\
	exit 1\n\
fi\n\
if [ $BD = 0  -a   $DS != 0 ]\n\
then\n\
	SLIP_DIAL_STRING=\"\"\n\
fi\n\
if [ $DS = 0 ]\n\
then\n\
	for i in \"$@\"\n\
	do\n\
		SLIP_DIAL_STRING=\"$i\"\n\
		shift\n\
	done\n\
fi\n\
export SLIP_DIAL_STRING\n\
TMP=`mkdev -c if -s SL -t sl -w sl$UNIT $PARM`\n\
if [ $? != 0 ]\n\
then\n\
	exit 1\n\
fi\n\
strinfo -m | grep slip>/dev/null\n\
SD=$?\n\
if [ $SD != 0 ]\n\
then\n\
	strload -m /usr/lib/drivers/slip\n\
fi\n\
SD=$?\n\
if [ $SD != 0 ]\n\
then\n\
	echo \"Cannot load slip discipline.\"\n\
	exit 1\n\
fi\n\
echo $TMP\n\
IP=`echo $TMP | awk '{ print $1 }'`\n\
TTY=`lsattr -E -l $IP -F \"value\" -a ttyport`\n\
if [ $UP = 0 ]\n\
then\n\
	if [ $BD != 0 ]\n\
	then\n\
		eval slattach $TTY\n\
		sleep 30\n\
	else\n\
		BD=`lsattr -E -l $IP -F \"value\" -a baudrate`\n\
		eval DST=\\\'$SLIP_DIAL_STRING\\\'\n\
		eval slattach $TTY $BD \\\'$DST\\\'\n\
		sleep 60\n\
	fi\n\
fi\n\
}\n\
cmdsl"
	ask			= "n"
	exec_mode		= "P"
	ghost			= "n"
	cmd_to_discover 	= ""
	cmd_to_discover_postfix   = ""
